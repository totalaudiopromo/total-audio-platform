{
  "name": "@total-audio/testing",
  "version": "1.0.0",
  "description": "Shared testing utilities for Total Audio Platform",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config/index.js",
    "./validators": "./dist/validators/index.js",
    "./helpers": "./dist/helpers/index.js",
    "./agents": "./dist/agents/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:enrichment": "vitest run src/enrichment"
  },
  "dependencies": {
    "@playwright/test": "^1.56.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/ui": "^2.1.8",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "testing",
    "playwright",
    "mobile",
    "accessibility",
    "wcag",
    "performance"
  ],
  "author": "Total Audio",
  "license": "MIT"
}
